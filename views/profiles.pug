html
    head 
    style.
        a
        {
            text-decoration:none;
            color:black;
        }
        h1 
        {
            text-align:center;
        }
        button {
           margin:10px;
           
            color: black;
            height:50px;
            width:100px;
            background-color:rgba(131, 236, 240, 0.5);;
        }
        div {
            margin-left:600px;
        }
        p 
        {
            text-align:center;
        }
    body
        h1 My Profile
        button 
            a(href='/home') Back
        div(style="display:flex") 
            each u in udata
                if(u.username==uu)
                    div(border="1px" style="margin:20px;padding:10px;border-style:solid;border-color:black;height:300px;width:250px" )
                        img(src=u.profilepic width="200px" height="200px"  style="border-radius: 50%;padding-left:20px" )
                        p="First name: "+u.username  
                        p="last Name: "+u.lastname
                            
        
        
                     
                        
                          
                           